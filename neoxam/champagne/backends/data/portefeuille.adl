SCHEMA PORTEFEUILLE
BEGIN
	%*****************************************************************************%
	%                                                                             %
	%    SCHEMA (PARTIE A INCLURE) UTILISE PAR LES OUTILS DU SERVICE ETUDES       %
	%    ET REALISATIONS.                                                         %
	%                                                     Version 1               %
	%    Developpe par L.CHAPUT et B.GENERE                                       %
	%    Date de creation le 27/12/1983                                           %
	%    Derniere modif   le 19/04/89           par C.BOUTTEAUX		      %
	%                                                                             %
	%    Nom du fichier XEXEC a inclure dans le schema : OSENEW.SMA               %
	%                                                                             %
	%    Notes:                                                                   %
	%      _ La version 3.1 de MAGNUM est necessaire                              %
	% cette version du schema OSER comporte de nouveaux champs dans la relation   %
	% OSE.STATISTIQUES.TRAITEMENTS : IO, MEMOIRE.UTILISEE, NUM.CPU, DEFAUT.PAGE   %
	%                                                                             %
	%*****************************************************************************%
	%%
	%***********************************************************************%
	%	GESTION DU SCHEMA						%
	%***********************************************************************%
	!INCLUDE FILE 'MAGNUM_LIB_OSER:OSESCH.SMA'
	%%
	%*****************************************************************************%
	%                                                                             %
	%    SCHEMA (PARTIE A INCLURE) UTILISE PAR LES OUTILS DU SERVICE ETUDES       %
	%    ET REALISATIONS. MODULE GESTION DE SCHEMA                                %
	%                                                     Version 3               %
	%    Developpe par C.DUCOLONE                                                 %
	%    Date de creation le 05/04/85                                             %
	%    Derniere modif   le 01/04/86           par B.Genere                      %
	%                                                                             %
	%    Nom du fichier VMS   a inclure dans le schema : OSESCH.SMA               %
	%                                                                             %
	%    Notes:                                                                   %
	%      _ La version 3.1 de MAGNUM est necessaire                              %
	%                                                                             %
	%*****************************************************************************%
	%%
	%*****************************************************************************%
	%               GENERATEUR DE SCHEMA                                          %
	%*****************************************************************************%
	FIELDS
	%****%
	OSE.NOM.OBJET AS '30C' % nom d'un objet ou d'une entite pour l'environnement(champ,relation,chaine ou application)%
	,
	OSE.NOM.CHAMP AS '30C' % nom de champ MAGNUM                                                           %
	,
	OSE.NOM.RELATION AS '30C' % nom d'une relation MAGNUM                                                     %
	,
	OSE.NOM.OBJET.PERE AS '30C'
	,
	OSE.NOM.OBJET.FILS AS '30C'
	,
	OSE.PLACE AS 'NB' % numero d'ordre                                                                %
	,
	OSE.STRUCTURE AS '20C' % format d'un champ                                                             %
	,
	OSE.SYNONYME AS '30C' % nom d'un champ same as                                                        %
	,
	OSE.TYPE.DONNEE AS 'C' verify ose.type.donnee eq 'C' or 'D' or 'I'
	with errors "C pour cle, D pour Data, I pour index"
	default "D"
	,
	OSE.TYPE.OBJET AS '30C' % nom d'un type d'objet %
	,
	OSE.TYPE.OBJET.FILS AS '30C'
	,
	OSE.STOCKAGE AS '50C' % clause stored in %
	,
	OSE.DEFAULT AS '50C' % clause default %
	,
	OSE.PROMPT AS '80C' % clause prompt %
	,
	OSE.ERROR AS '80C' % clause erreur %
	,
	OSE.TEMOIN.VERIFY AS 'C' VERIFY OSE.TEMOIN.VERIFY EQ 'O' OR 'N'
	default 'N'
	,
	OSE.CODE.VERSION AS '12C' % numero de version de l'executable % ! cjo 06/12/11
	,
	OSE.COMMENTAIRE AS '80C' % commentaire court %
	,
	OSE.DATE.VALIDATION AS DATE "DD/MM/YB" ! cjo 18/01/00
	,
	OSE.DATE.DEVALIDATION AS DATE "DD/MM/YB" ! cjo 18/01/00
	,
	OSE.NATURE.OPTION AS "6C" ! CJO 19/03/02
	,
	OSE.NOM.SERVEUR AS "25C" % Code du serveur % ! CJO 06/12/11
	%%
	% RELATIONS DE BASE %
	RELATION OSE.RELATION IS
	%******%
	KEY
	OSE.NOM.RELATION
	DATA
	OSE.SYNONYME
	, OSE.STOCKAGE
	RELATION OSE.CHAMP IS
	%******%
	KEY
	OSE.NOM.CHAMP
	DATA
	OSE.STRUCTURE
	, OSE.SYNONYME
	RELATION OSE.CHAMP.COMPLEMENT IS
	%******%
	KEY
	OSE.NOM.CHAMP
	DATA
	OSE.DEFAULT
	, OSE.PROMPT
	, OSE.ERROR
	, OSE.TEMOIN.VERIFY
	%%
	% RELATIONS DE COMPOSITION %
	RELATION OSE.COMPOSITION.OBJET IS
	%******%
	KEY
	OSE.NOM.OBJET.PERE
	, OSE.NOM.OBJET.FILS
	DATA
	OSE.PLACE
	RELATION OSE.COMPOSITION.RELATION IS
	%******%
	KEY
	OSE.NOM.RELATION
	, OSE.TYPE.DONNEE
	, OSE.NOM.CHAMP
	DATA
	OSE.PLACE
	%%
	% RELATIONS DE DOCUMENTATION%
	RELATION OSE.OBJET IS
	%======%
	KEY
	OSE.NOM.OBJET
	DATA
	OSE.COMMENTAIRE
	, OSE.TYPE.OBJET
	RELATION OSE.TABLE.TYPE.OBJET IS
	%======%
	KEY
	OSE.TYPE.OBJET
	DATA
	OSE.COMMENTAIRE
	, OSE.TYPE.OBJET.FILS
	%%
	%*****************************************************************************%
	%               GESTION DES FICHIERS                                          %
	%*****************************************************************************%
	FIELDS
	%****%
	FICHIER. AS '50C' % nom de fichier XEXEC avec controle de la valeur %
	VERIFY ( LENGTH ( FICHIER. ) - POSITION ( FICHIER. , ":" ) NE 0
	AND POSITION ( FICHIER. , "!" ) = 0
	AND POSITION ( FICHIER. , '"' ) = 0
	AND POSITION ( FICHIER. , "#" ) = 0
	AND POSITION ( FICHIER. , "%" ) = 0
	AND POSITION ( FICHIER. , "&" ) = 0
	AND POSITION ( FICHIER. , "'" ) = 0
	AND POSITION ( FICHIER. , "=" ) = 0
	AND POSITION ( FICHIER. , "~" ) = 0
	AND POSITION ( FICHIER. , "|" ) = 0
	AND POSITION ( FICHIER. , "-" ) = 0
	AND POSITION ( FICHIER. , "^" ) = 0
	AND POSITION ( FICHIER. , "\" ) = 0
	AND POSITION ( FICHIER. , "{" ) = 0
	AND POSITION ( FICHIER. , "}" ) = 0
	AND POSITION ( FICHIER. , "[" ) = 0
	AND POSITION ( FICHIER. , "]" ) = 0
	AND POSITION ( FICHIER. , "+" ) = 0
	AND POSITION ( FICHIER. , "*" ) = 0
	AND POSITION ( FICHIER. , "`" ) = 0
	AND POSITION ( FICHIER. , ";" ) = 0
	AND POSITION ( FICHIER. , "@" ) = 0
	AND POSITION ( FICHIER. , "<" ) = 0
	AND POSITION ( FICHIER. , ">" ) = 0
	AND POSITION ( FICHIER. , "?" ) = 0
	AND POSITION ( FICHIER. , "," ) = 0
	AND POSITION ( FICHIER. , "/" ) = 0
	AND POSITION ( FICHIER. , ")" ) = 0
	AND POSITION ( FICHIER. , "(" ) = 0
	AND POSITION ( FICHIER. , "." ) # 0
	AND POSITION ( TRIM ( FICHIER. ) , " " ) EQ 0
	AND NOT ( POSITION ( FICHIER. , "." ) NE 0
	AND POSITION ( FICHIER. , "." ) LT LENGTH ( FICHIER. ) - 10 )
	AND POSITION ( FICHIER. , "." , POSITION ( FICHIER. , "." ) + 1 ) EQ 0
	AND POSITION ( FICHIER. , ":" , POSITION ( FICHIER. , ":" ) + 1 ) EQ 0
	AND POSITION ( FICHIER. , "." ) GE POSITION ( FICHIER. , ":" )
	AND ( POSITION ( FICHIER. , ":" ) EQ 0 OR POSITION ( FICHIER. , ":" ) GT 6 )
	AND ( POSITION ( FICHIER. , "." ) - POSITION ( FICHIER. , ":" ) ) GT 1
	AND POSITION ( FICHIER. , ":" ) LE 26
	)
	OR ( FICHIER. EQ 'TERMINAL' OR 'TERMINA' )
	%%
	%*****************************************************************************%
	%               GESTION DU DIFFERE ET DES DIALOGUES                           %
	%*****************************************************************************%
	FIELDS
	%****%
	DEMANDE AS '254C' % chaine qui contient l'ensemble des reponses entrees a l'avance %
	,
	HEURE AS '8C' % heure sous la forme HH:MM:SS                                   %
	,
	JOUR AS DATE 'YYMMDD' %                                                                %
	,
	OSE.QUESTION SAME AS DEMANDE % chaine contenant un libelle de cle				%
	,
	LISTE.QUESTION SAME AS DEMANDE % chaine qui contient l'ensemble des questions du dialogue       %
	,
	LU SAME AS DEMANDE % chaine qui contient l'ensemble des reponses enregistrees       %
	,
	REPONSE SAME AS DEMANDE % chaine tampon recueillant la reponse a une question            %
	,
	REPONSE.SUITE SAME AS DEMANDE % chaine tampon recueillant les reponses anticipees              %
	,
	OSE.USER.NAME AS "12C"
	,
	OSE.MESSAGE SAME AS DEMANDE % message			%
	%%
	%*****************************************************************************%
	%               GESTION DES MENUS APPELS DE PROCEDURE                         %
	%*****************************************************************************%
	FIELDS
	%****%
	OSE.CODE.MENU AS "8C"
	,
	OSE.CODE.OPTION AS "8C"
	,
	OSE.NUMERO.OPTION AS "NB" % Place de l'option dans le menu %
	,
	OSE.TYPE.OPTION AS "C" % (M)enu ou (P)rogramme %
	VERIFY OSE.TYPE.OPTION EQ 'M' OR 'P'
	DEFAULT 'P'
	,
	OSE.CODE.PROCEDURE AS "8C"
	,
	OSE.CODE.UTILISATEUR AS "6C"
	,
	OSE.CODE.UTILISATEUR.TYPE AS "6C" ! cjo 18/01/00
	,
	OSE.NOM.PROCEDURE AS "30C"
	,
	OSE.NOM.APPEL AS "8C" ! cjo 08/11/99
	,
	OSE.FONCTION AS '10C' % Fonction de l'utilisateur                                     %
	,
	OSE.SERVICE AS '15C' % Libelle d'un service utilisateur                              %
	,
	OSE.SOCIETE AS '15C' % Libelle d'une societe utilisatrice                            %
	,
	OSE.CENTRE AS '15C' % centre de l'utilisateur                                       %
	,
	OSE.NOM.UTILISATEUR AS '20C' % libelle de l'utilisateur %
	,
	OSE.NOM.SOCIETE AS '20C' % libelle de la societe %
	,
	OSE.MOT.PASSE AS "10C"
	,
	OSE.IDENTIFIANT AS "5C"
	,
	OSE.TITRE.MENU AS "50C"
	,
	OSE.TITRE.PROGRAMME AS "50C" % Libelle decrivant le programme                                %
	,
	TITRE.APPLICATION AS '50C' % Libelle de l'application                                      %
	%%
	RELATION OSE.UTILISATEUR IS % table des USER.NAMEs avec indication en clair de l'utilisateur        %
	%======%
	KEY
	OSE.USER.NAME
	, OSE.IDENTIFIANT
	DATA
	OSE.CODE.UTILISATEUR
	INDEX ON
	OSE.CODE.UTILISATEUR
	RELATION OSE.INFO.UTILISATEUR IS
	%======%
	KEY
	OSE.CODE.UTILISATEUR
	DATA
	OSE.SOCIETE
	, OSE.SERVICE
	, OSE.CENTRE
	, OSE.FONCTION
	, OSE.NOM.UTILISATEUR
	, OSE.NOM.SOCIETE
	, OSE.MOT.PASSE
	, OSE.CODE.MENU
	, OSE.DATE.VALIDATION ! cjo 18/01/00
	, OSE.DATE.DEVALIDATION ! cjo 18/01/00
	RELATION OSE.UTILISATEUR.TYPE IS ! cjo 18/01/00
	%======%
	KEY
	OSE.CODE.UTILISATEUR.TYPE
	DATA
	OSE.NOM.UTILISATEUR
	, OSE.CODE.MENU
	RELATION OSE.OPTION.POSSIBLE IS % table d'acces aux menus suivant l'utilisateur                         %
	%======%
	KEY
	OSE.CODE.UTILISATEUR
	, OSE.CODE.OPTION
	! cjo 19/11/99
	RELATION OSE.MAJ.IMPOSSIBLE IS % table d'acces aux options pour lesquelles la mise a jour est impossible %
	%======%
	KEY
	OSE.CODE.UTILISATEUR
	, OSE.CODE.OPTION
	RELATION OSE.MENU IS % table des menus                                        %
	%======%
	KEY
	OSE.CODE.MENU
	DATA
	OSE.TITRE.MENU
	, OSE.NOM.APPEL
	INDEX ON
	OSE.NOM.APPEL
	RELATION OSE.COMPOSITION.MENU IS % table de composition des menus                                        %
	%======%
	KEY
	OSE.CODE.MENU
	, OSE.NUMERO.OPTION
	DATA
	OSE.CODE.OPTION
	, OSE.TYPE.OPTION
	RELATION OSE.PROGRAMME IS % table des programmes avec titre et nom de la procedure MAGNUM         %
	%======%
	KEY
	OSE.CODE.PROCEDURE
	DATA
	OSE.NOM.PROCEDURE
	, OSE.TITRE.PROGRAMME
	, OSE.NOM.APPEL
	, OSE.NATURE.OPTION
	INDEX ON
	OSE.NOM.APPEL
	RELATION OSE.VALEURS.PAR.DEFAUT IS % pour un utilisateur et un programme donne reponses a fournir aux questions %
	%======%
	KEY
	OSE.CODE.UTILISATEUR
	, OSE.CODE.PROCEDURE
	DATA
	DEMANDE
	!INCLUDE FILE 'MAGNUM_LIB_OSER:OSEBIBLI.SMA'
	!INCLUDE FILE 'MAGNUM_LIB_OSER:OSECOH.SMA'
	!INCLUDE FILE 'MAGNUM_LIB_OSER:OSESTA.SMA'
	FIELDS
	%====%
	OSE.DESCRIPTION AS "60C"
	, OSE.NOM.FONCTION AS "30C"
	, OSE.TYPE.FONCTION AS "C"
	DEFAULT 'M'
	VERIFY OSE.TYPE.FONCTION EQ 'M' OR 'S' OR 'F'
	, OSE.NOM.PARAMETRE AS "30C"
	, OSE.PLACE.PARAMETRE AS "2N"
	, OSE.TYPE.LOCALISATION AS "C"
	DEFAULT 'F'
	VERIFY OSE.TYPE.LOCALISATION EQ 'F' OR 'T'
	, OSE.TYPE.PARAMETRE AS "C"
	DEFAULT 'F'
	VERIFY OSE.TYPE.PARAMETRE EQ 'F' OR 'E' OR 'S' OR 'X'
	RELATION OSE.COMPOSITION.FONCTION IS
	%======%
	KEY
	OSE.NOM.FONCTION
	, OSE.NOM.PARAMETRE
	DATA
	OSE.DESCRIPTION
	, OSE.TYPE.PARAMETRE
	, OSE.PLACE.PARAMETRE
	RELATION OSE.DETAIL.FONCTION IS
	%======%
	KEY
	OSE.NOM.FONCTION
	DATA
	OSE.TYPE.LOCALISATION
	, OSE.STOCKAGE
	, OSE.TYPE.FONCTION
	, OSE.SYNONYME
	%%
	%*****************************************************************************%
	%                                                                             %
	%    SCHEMA (PARTIE A INCLURE) UTILISE PAR LES OUTILS DU SERVICE ETUDES       %
	%    ET REALISATIONS. MODULE CONTROLE DE COHERENCE			      %
	%                                                     Version 1               %
	%    Developpe par B.GENERE  						      %
	%    Date de creation le 17/12/85                                             %
	%    Derniere modif   le                    par                               %
	%                                                                             %
	%    Nom du fichier DCL     inclure dans le schema : OSECOH.SMA               %
	%                                                                             %
	%    Notes:                                                                   %
	%                                                                             %
	%*****************************************************************************%
	%%
	FIELDS
	%****%
	OSE.ENONCE AS '254C' % ENONCE de regle									%
	,
	OSE.NUMERO.REGLE AS '2ZN' % numero de la regle								%
	%%
	% RELATIONS DE BASE %
	RELATION OSE.REGLE IS
	%******%
	KEY
	OSE.NOM.RELATION
	, OSE.NUMERO.REGLE
	DATA
	OSE.ENONCE
	, OSE.MESSAGE
	%%
	% DATE : 19/04/89 CETTE NOUVELLE VERSION DU SCHEMA STATISTIQUE V4 OSER    %
	%                 APPORTE DE NOUVEAUX CHAMPS : NUM.CPU, IO, DEFAUT.PAGE   %
	% ET MEMOIRE UTILISEE                                                     %
	FIELDS
	%****%
	!!!! cjo 08/11/99 OSE.NUM.REQUETE			AS '-6N'
	OSE.NUM.REQUETE AS '-8ZN'
	,
	OSE.ETAT AS 'C'
	VERIFY OSE.ETAT EQ 'C' OR 'T' OR 'F'
	,
	OSE.DATE.DEBUT SAME AS JOUR
	,
	OSE.DATE.FIN SAME AS JOUR
	,
	OSE.HEURE.DEBUT SAME AS HEURE
	,
	OSE.HEURE.FIN SAME AS HEURE
	,
	OSE.CPU.CONSOMMEE AS ' 6ZN.NN'
	,
	OSE.MODE.EXECUTION AS '11C'
	,
	OSE.IO.DIRECT AS '7ZN'
	,
	OSE.MEMOIRE.UTILISEE AS '9ZN'
	,
	OSE.NUM.CPU AS '5C'
	,
	OSE.DEFAUT.DE.PAGE AS '8ZN'
	,
	OSE.CPU.SERVEUR AS ' 6ZN.NN'
	,
	OSE.PROCESS.ID AS '11C'
	RELATION OSE.STATISTIQUES.TRAITEMENTS IS
	%******%
	KEY
	OSE.NUM.REQUETE
	DATA
	OSE.ETAT
	, OSE.DATE.DEBUT
	, OSE.DATE.FIN
	, OSE.HEURE.DEBUT
	, OSE.HEURE.FIN
	, OSE.CPU.CONSOMMEE
	, OSE.CODE.UTILISATEUR
	, OSE.CODE.PROCEDURE
	, OSE.MODE.EXECUTION
	, OSE.IO.DIRECT
	, OSE.MEMOIRE.UTILISEE
	, OSE.DEFAUT.DE.PAGE
	, OSE.NUM.CPU
	, OSE.CODE.VERSION ! cjo 06/12/11
	, OSE.NOM.SERVEUR ! cjo 06/12/11
	%*****************************************************************************%
	%       derniere       EXECUTION DES PROGRAMMES                               %
	%*****************************************************************************%
	FIELDS
	%****%
	JOUR.FIN SAME AS JOUR %  %
	,
	HEURE.FIN SAME AS HEURE %  %
	%*****************************************************************************%
	%       GESTION DES SOURCES                                  TD - 27/12/95    %
	%*****************************************************************************%
	FIELDS
	%****%
	OSE.NOM.VERSION AS '6C' %     - Nom de la version PORTFOLIO            %
	, OSE.NOM.SOURCE SAME AS OSE.NOM.OBJET % 30C - Nom du source ( texte MAGNUM )         %
	, OSE.CODE.PRODUIT AS '6C' %     - Sous produit PORTFOLIO : PC, PI ou GP  %
	, OSE.TYPE.SOURCE AS '6C' %     - Forme technique : PROC, RUN, BIB       %
	, OSE.FONCTION.SOURCE AS '6C' %     - Action fonctionnelle : etat, maj...    %
	, OSE.FLAG.FRAME AS '2C' %     - 0: pas de frame, 1: normal, 2: multi   %
	, OSE.NOMBRE.FRAME AS 'ZN' %     - Nombre de frame                        %
	, OSE.FLAG.MOTEUR AS '2C' %     - O : la maj appelle le moteur de gestion%
	, OSE.FLAG.ETAT AS '2C' %     - O : etat ( un des 3 cas suivant )      %
	, OSE.FLAG.ETAT.ECRAN AS '2C' %     - OC : consult XBO, OT : consult tableau %
	, OSE.FLAG.ETAT.FICHIER AS '2C' %     - O : donnees exportees sous fichier     %
	, OSE.FLAG.ETAT.PAPIER AS '2C' %     - O : etat normal                        %
	, OSE.NOM.VMS AS '30C' %     - Nom VMS                                %
	, OSE.NOM.VERSION.REF SAME AS OSE.NOM.VERSION %     - Version reference ( precedente ou '' ) %
	, OSE.NOM.SOURCE.REF SAME AS OSE.NOM.SOURCE %     - Source reference                       %
	, OSE.STATUT.SOURCE AS '6C' %     - Validite du source dans la version     %
	, OSE.ID.STATUT SAME AS OSE.CODE.UTILISATEUR % 6C  - Identifiant du validateur              %
	, OSE.DATE.STATUT AS DATE 'DB/MM/YY' %     - Date de validation                     %
	, OSE.HEURE.STATUT SAME AS HEURE % 8C  - Heure de validation ( en HH:MM:SS )    %
	, OSE.STATUT.GRAPHIQUE AS '6C' %     - Coherence avec ressource graphique     %
	, OSE.ID.STATUT.GRAPHIQUE SAME AS OSE.CODE.UTILISATEUR % 6C  - Identifiant du validateur              %
	, OSE.DATE.STATUT.GRAPHIQUE AS DATE 'DB/MM/YY' %     - Date de verif du RC                    %
	, OSE.HEURE.STATUT.GRAPHIQUE SAME AS HEURE % 8C  - Heure de validation ( en HH:MM:SS )    %
	, OSE.LIB.STATUT AS '60C' %     - Commentaire libre                      %
	, OSE.TYPE.TRADUCTION AS '2C' %     - TE/TI/NT: traduction ext, int, non prev%
	, OSE.STATUT.TRADUCTION AS '2C' %     -                                        %
	RELATION OSE.SOURCE IS
	KEY
	OSE.NOM.SOURCE
	DATA
	OSE.CODE.PRODUIT
	, OSE.NOM.VMS
	, OSE.TYPE.SOURCE
	, OSE.FONCTION.SOURCE
	! Champs non encore gérés - TD 08/04/99
	, OSE.NOMBRE.FRAME
	, OSE.FLAG.MOTEUR
	, OSE.FLAG.FRAME
	, OSE.FLAG.ETAT
	, OSE.FLAG.ETAT.ECRAN
	, OSE.FLAG.ETAT.FICHIER
	, OSE.FLAG.ETAT.PAPIER
	RELATION OSE.COMPOSITION.VERSION IS
	KEY
	OSE.NOM.VERSION
	, OSE.NOM.SOURCE
	DATA
	OSE.STATUT.SOURCE
	, OSE.DATE.STATUT
	, OSE.HEURE.STATUT
	, OSE.ID.STATUT
	, OSE.STATUT.GRAPHIQUE
	, OSE.ID.STATUT.GRAPHIQUE
	, OSE.DATE.STATUT.GRAPHIQUE
	, OSE.HEURE.STATUT.GRAPHIQUE
	, OSE.NOM.VERSION.REF
	, OSE.NOM.SOURCE.REF
	, OSE.LIB.STATUT
	, OSE.TYPE.TRADUCTION
	, OSE.STATUT.TRADUCTION
	%                                                T E X T E  D U  S C H E M A
                                                 ===========================
%
	%        CHAMPS : LETTRE                                                                                               PAGE 1     %
	%        SCHEMA                                                                                                       LE 12/07/06 %
	FIELDS
	%====%
	% A.dern.parite.vl.multiple    voir le champ Parite                        %
	AO.expression.cours AS "C"
	% Expression cours utilisee dans l'avis d'opere %
	,
	AO.expression.quantite AS "C"
	% Expression quantite utilisee dans l'avis d'opere %
	,
	% AO.nominal                   voir le champ Nominal                       %
	Actif.jour.veille AS "C"
	% Frais de gestion sur actif jour ou veille %
	,
	% Actif.net.dv                 voir le champ Montant                       %
	% Actif.net.moyen              voir le champ Montant                       %
	% Actif.net.part               voir le champ Montant                       %
	% Actif.net.reference          voir le champ Montant                       %
	% Actif.net.tot                voir le champ Montant                       %
	% Actif.part.avant.frais       voir le champ Montant                       %
	% Actif.passif                 voir le champ Type.                         %
	Admis.srd AS "C"
	% Valeur admise au SRD %
	,
	Adresse.1 AS "32C"
	% 1ere partie de l'adresse du tiers %
	,
	Adresse.2 AS "32C"
	% 2eme partie de l'adresse du tiers %
	,
	Adresse.3 AS "32C"
	% 3eme partie de l'adresse du tiers %
	,
	Adresse.4 AS "32C"
	% 4eme partie de l'adresse du tiers %
	,
	% Adresse.destinataire         voir le champ Bic                           %
	Adresse.diffusion AS "80C"
	% Coordonnes de diffusion d'un destinataire de reporting %
	,
	Adresse.immeuble AS "40C"
	% Adresse de l'immeuble %
	,
	% Adresse.emetteur             voir le champ Bic                           %
	Apparentement AS "C"
	%        CHAMPS : LETTRE A                                                                                             PAGE 2     %
	%        SCHEMA                                                                                                       LE 12/07/06 %
	,
	% Autorisation.decouvert       voir le champ Booleen                       %
	Autorisation.decouvert.opc AS 'C'
	% Autorisation decouvert d'un titre OPC %
	,
	% Autorisation.decouvert.supporvoir le champ Booleen                       %
	% Autorite.agrement            voir le champ Code.intervenant              %
	% Avec.assurance               voir le champ Booleen                       %
	% Avec.banque                  voir le champ Booleen                       %
	% Avoir.fiscal                 voir le champ Cours.sur.9n                  %
